<% content_for :head do %>
  <%= javascript_import_module_tag "custom/bg-animation" %>
<% end %>

<div id='home'>
  <%= render 'components/header' %>

  <h2>De seneste udviklere</h2>

  <div class='row'>
    <% @developers.each do |developer| %>
      <div class='col-md-6 py-3'>
        <%= render 'developers/card_sm', developer: developer %>
      </div>
    <% end %>
  </div>
</div>


<script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.18.0/TweenMax.min.js"></script>
